{
  "vendor": "jillian",
  "name": "search-graphql",
  "version": "1.62.0",
  "title": "[Custom] GraphQL API for the VTEX store APIs",
  "description": "Forked from vtex.search-graphql@0.62.0. Main changes: Add CHEAPEST_AVAILABLE to type ItemsFilter in graphql/types/Product.graphql.",
  "credentialType": "absolute",
  "builders": {
    "graphql": "1.x",
    "docs": "0.x"
  },
  "dependencies": {
    "vtex.messages": "1.x",
    "vtex.catalog-api-proxy": "0.x"
  },
  "policies": [
    {
      "name": "vtex.messages:translate-messages"
    },
    {
      "name": "vtex.catalog-api-proxy:catalog-proxy"
    },
    {
      "name": "vbase-read-write"
    },
    {
      "name": "colossus-fire-event"
    }
  ],
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "type": "free",
    "availableCountries": ["*"]
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
